class Solution {
public:
    int maxFreqSum(string s) {
        int freq[26]={0};
        int Vmax=0,Cmax=0;
        for(char c : s){
            freq[c-'a']+=1;
            if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'){
                Vmax = max(Vmax,freq[c-'a']);
            }
            else{
                Cmax = max(Cmax,freq[c-'a']);
            }
        }
        return Vmax+Cmax;
    }
};