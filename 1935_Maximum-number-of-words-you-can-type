class Solution {
public:
    int canBeTypedWords(string text, string brokenLetters) {
        int count = 0;
        istringstream iss(text);
        string  word;
        while(iss >> word){
            bool Type = true;
            for(char c : brokenLetters){
                if(word.find(c)!=string::npos){
                    Type = false;
                    break;
                }
            }
            if(Type) count++;
        }
        return count;
    }
};