class Solution {
public:
    vector<int> getNoZeroIntegers(int n) {
        for(int i=1 ; i<n ; i++){
            int b = n - i;
            if(isValid(i) && isValid(b)){
                return {i,b};
            }
        }
        return {};
    }
private:
    bool isValid(int num){
        while(num>0){
            if(num%10==0) return false;
            num/=10;
        }
        return true;
    }
};